{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "slim/slim": "3.*",
    "doctrine/orm": "~2.5",
    "doctrine/migrations": "~1.5",
    "monolog/monolog": "~1.18",
    "hassankhan/config": "~0.10",
    "symfony/validator": "~3.0",
    "slim/flash": "~0.1",
    "slim/csrf": "~0.6",
    "guzzlehttp/guzzle": "~6.2",
    "slim/twig-view": "~2.2",
    "beberlei/DoctrineExtensions": "~1.0",
    "gedmo/doctrine-extensions": "~2.4",
    "tuupola/cors-middleware": "~0.5",
    "tuupola/slim-jwt-auth": "~2.3",
    "vlucas/phpdotenv": "~2.4",
    "nesbot/carbon": "~1.21",
    "peppeocchi/php-cron-scheduler": "~1.0",
    "runcmf/runtracy": "~0.2",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*"
  },
  "require-dev": {
    "symfony/yaml": "2.5.*@dev",
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "1.*",
    "facebook/webdriver": "~1.0",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0.0",
    "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0.0"
  },
  "bin": [
    "composer/bin/dbunit"
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "framework\\",
      "Entity\\": "entity\\",
      "Blog\\": "app\\Blog",
      "CMS\\": "app\\CMS",
      "Task\\": "task\\"
    },
    "files": [
      "framework\\Lib\\functions_include.php"
    ]
  }
}
