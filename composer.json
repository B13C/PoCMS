{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "slim/slim": "3.*",
    "slim/twig-view": "*",
    "doctrine/common": "*",
    "doctrine/orm": "*",
    "doctrine/collections": "~1.2",
    "doctrine/dbal": ">=2.5-dev,<2.6-dev",
    "doctrine/instantiator": "~1.0.1",
    "doctrine/cache": "~1.4",
    "monolog/monolog": "~1.18",
    "davidepastore/slim-config": "*",
    "slim/flash": "~0.1",
    "slim/csrf": "~0.6",
