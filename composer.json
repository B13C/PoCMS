{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "slim/slim": "3.*",
    "doctrine/orm": "*",
    "doctrine/migrations": "*",
    "monolog/monolog": "~1.18",
    "hassankhan/config": "*",
    "symfony/validator": "~3.0",
    "slim/flash": "~0.1",
    "slim/csrf": "~0.6",
    "guzzlehttp/guzzle": "*",
    "slim/twig-view": "*",
    "beberlei/DoctrineExtensions": "*",
    "gedmo/doctrine-extensions": "*",
    "tuupola/cors-middleware": "*",
    "tuupola/slim-jwt-auth": "*",
    "vlucas/phpdotenv": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*"
  },
  "require-dev": {
    "symfony/yaml": "2.5.*@dev",
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "1.*",
    "facebook/webdriver": "~1.0",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0.0",
    "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0.0"
  },
  "bin": [
    "composer/bin/dbunit"
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "framework\\",
      "Entity\\": "entity\\",
      "Blog\\": "app\\Blog",
      "CMS\\": "app\\CMS",
      "Task\\": "task\\"
    },
    "files": [
      "framework\\Lib\\functions_include.php"
    ]
  }
}
